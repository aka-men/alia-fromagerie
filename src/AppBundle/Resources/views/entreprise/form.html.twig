    <div class="col-lg-3 ccol-md-3 col-sm-6 col-xs-12">
        <div class="form-group">
            {{ form_label(formEntreprise.idCE) }}
            {{ form_widget(formEntreprise.idCE, {'attr': {'class':'form-control entier','data-parsley-type':"integer"}}) }}
