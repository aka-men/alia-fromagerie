<div class="row">
    <div class="col-lg-12">
        <p style="padding: 10px 15px;color: red;" class="form-error"></p>
    </div>
</div>
{{ form_start(formEntreprise,{ 'action': action,'attr':{'method':'POST','id':'appbundle_entreprise'}}) }}
<div class="row">
    <div class="col-lg-3 ccol-md-3 col-sm-6 col-xs-12">
        <div class="form-group">
            {{ form_label(formEntreprise.nom) }}
            {{ form_widget(formEntreprise.nom, {'attr': {'class':'form-control'}}) }}
        </div>
    </div>
    <div class="col-lg-3 ccol-md-3 col-sm-6 col-xs-12">
        <div class="form-group">
            {{ form_label(formEntreprise.email) }}
            {{ form_widget(formEntreprise.email, {'attr': {'class':'form-control','data-parsley-type':"email"}}) }}
